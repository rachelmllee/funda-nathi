<p id="notice"><%= notice %></p>

<div class="brick">
  <section id="services">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 text-center">
          <h2 class="section-heading">Your Chats</h2>
          <h3 class="section-subheading text-muted"> </h3>
          <%= link_to 'Start New Chat', new_chat_path, class: 'btn btn-lg btn-primary' %>
        </div>
      </div>
    </div>
    <div class="space"></div>
    <div class="container">
      <div class="row">

        <div class="col-sm-12">

          <ul class="list-group">
            <%= render partial: 'chats/chat', collection: @chats %>
          </ul>

        </div>
      </div>
    </div>
</div>
</section>
</div>
